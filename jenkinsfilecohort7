pipeline{
  agent any
  tools {
    maven "3.8.6 maven"
  }
  stages{
    stage('Git-Clone'){
      steps{
        git credentialsId: 'Github-Cred', url: 'https://github.com/team16flight/web-app.git'
      }
    }
    stage('Build'){
      steps{
        common("Build")
      }
    }
    stage('Execute-SonarQube-Report'){
      steps{
        common("SonarQube Report")
      }
    }
    stage('BackUp'){
      steps{
        common("Upload Into Nexus")
      }
    }
  }
}
